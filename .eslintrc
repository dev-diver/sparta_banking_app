{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "eslint-config-prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier", "node"],
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": "error",
    "node/no-unsupported-features/es-syntax": ["error", { "ignores": ["modules"] }]
  }
}